# This is Git's per-user configuration file.
[user]
	name = jonbwalker
	email = jwalker@sendlane.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	add-commit = !git add -A && git commit
[core]
	autocrlf = input
